# Frontend .env.example â€” copy to .env.local for local development.
# IMPORTANT: never commit real RPC keys, Privy secrets, or paymaster endpoints.

# Backend endpoints
# - Local dev (recommended): leave these unset and use the Vite proxy
# - Remote backend: set full URLs
VITE_BACKEND_URL=http://localhost:8000
VITE_API_BASE_URL=http://localhost:8000/api
VITE_SOCKET_URL=http://localhost:8000

# Network / RPC (used by wallet/onchain helpers)
VITE_CHAIN_ID=84532
VITE_RPC_URL=https://YOUR_RPC_HTTP
VITE_RPC_WSS_URL=wss://YOUR_RPC_WSS

# Auth / Privy (optional, identity-only)
VITE_PRIVY_APP_ID=YOUR_PRIVY_APP_ID

# Paymaster / AA (planned; keep unset for identity-only)
VITE_PUBLIC_PAYMASTER_AND_BUNDLER_ENDPOINT=

# Feature flags
VITE_ENABLE_WALLET=false