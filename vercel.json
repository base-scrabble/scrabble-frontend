{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/", "destination": "/waitlist" },
    { "source": "/success", "destination": "/success" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
