{
    "version": 2,
    "routes": [
        {
            "src": "^/frames/(.*)$",
            "headers": {
                "cache-control": "no-store"
            },
            "continue": true
        },
        {
            "src": "^/\\.well-known/farcaster\\.json$",
            "headers": {
                "cache-control": "no-store"
            },
            "continue": true
        },
        {
            "src": "^/waitlist$",
            "dest": "/waitlist/index.html"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "^/api/(.*)$",
            "dest": "/api/$1"
        },
        {
            "src": "^(?!/api/)(.*)$",
            "dest": "/index.html"
        }
    ]
}
