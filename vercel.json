{
    "version": 2,
    "routes": [
        { "src": "/frames/(.*)", "dest": "/frames/$1" },
        {
            "src": "/.well-known/farcaster.json",
            "dest": "/.well-known/farcaster.json",
            "headers": {
                "Content-Type": "application/json; charset=utf-8",
                "Cache-Control": "no-store"
            }
        },
        { "src": "/", "dest": "/waitlist" },
        { "src": "/success", "dest": "/success" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
