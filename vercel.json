{
    "version": 2,
    "routes": [
        {
            "src": "^/terms$",
            "dest": "/terms.html"
        },
        {
            "src": "^/privacy$",
            "dest": "/privacy.html"
        },
        {
            "src": "^/frames/(.*)$",
            "headers": {
                "cache-control": "no-store"
            },
            "continue": true
        },
        {
            "src": "^/\\.well-known/farcaster\\.json$",
            "headers": {
                "cache-control": "no-store"
            },
            "continue": true
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "^/api/(.*)$",
            "dest": "/api/$1"
        },
        {
            "src": "^/(.*)$",
            "dest": "/index.html"
        }
    ]
}
